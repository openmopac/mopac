$(OBJ)/parameters_for_AM1_Sparkles_C.o  $(OBJ)/parameters_for_PM7_C.o  $(OBJ)/post_scf_corrections.o      \
$(OBJ)/parameters_for_PM8_C.o \
$(OBJ)/local_for_MOZYME.o         $(OBJ)/add_hydrogen_atoms.o       $(OBJ)/wrt_diffs.o                 \
$(OBJ)/dftd3.o                    $(OBJ)/parameters_for_PM7_Sparkles_C.o                               \
$(OBJ)/parameters_for_PM7_TS_C.o  $(OBJ)/parameters_for_RM1_Sparkles_C.o                               \
$(OBJ)/dftd3_bits.o               $(OBJ)/copyc6.o		    $(OBJ)/mxv.o                       \
$(OBJ)/mxv_I.o                    \

#
# Define targets
#


#
# Make MOPAC executable for Linux     
#

all: MOPAC2016


MOPAC2016: $(OBJ) $(OBJS) 
ifneq ($(GPU), Kepler)
	@echo :
	@echo : "Building Fermi (Good for Kepler and Fermi)"
	@echo :
else
	@echo :
	@echo :  "Building Kepler (Good for Kepler only)"
	@echo :
endif
	$(FC_LINK) $(FC_Flags) -o /opt/mopac/MOPAC2016.exe $(OBJS) $(MATH_LIBS) $(MATH_LINK)
ifneq ($(GPU), Kepler)
	@echo :
	@echo : "Fermi built (Good for Kepler and Fermi)"
	@echo :
else
	@echo :
	@echo :  "Kepler built (Good for Kepler only)"
	@echo :
endif


# Directory for object files
$(OBJ): 
	mkdir $(OBJ)


# Clean up all
clean: 
	rm -f $(LINUX_DIR)/$(OBJ)/*.o $(LINUX_DIR)/$(OBJ)/*.mod $(LINUX_DIR)/modules/*.mod $(LINUX_DIR)/*.o


